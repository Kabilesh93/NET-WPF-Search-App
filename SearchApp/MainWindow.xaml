<Window x:Class="SearchApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SearchApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="50" Margin="90,30,0,0" TextWrapping="Wrap" Text="Enter the question" VerticalAlignment="Top" Width="400" Name="QuestionBox"/>
        <Button Content="SearchGoogle" HorizontalAlignment="Left" Margin="199,110,0,0" VerticalAlignment="Top" Width="128" Height="27" Click="SearchGoogle_Click"/>
        <Button Content="SearchWiki" HorizontalAlignment="Left" Margin="362,110,0,0" VerticalAlignment="Top" Width="128" Height="27" Click="SearchWiki_Click"/>
        <TextBox HorizontalAlignment="Left" Height="100" Margin="90,200,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="400" Name="AnswerBox"/>
        
    </Grid>
</Window>
